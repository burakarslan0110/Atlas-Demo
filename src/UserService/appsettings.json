{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=POSTGRES_HOST;Database=atlas_user;Username=POSTGRES_USER;Password=POSTGRES_PASSWORD",
    "Redis": "REDIS_HOST:6379"
  },
  "Jwt": {
    "Secret": "JWT_SECRET_PLACEHOLDER_MUST_BE_AT_LEAST_32_CHARACTERS_LONG",
    "Issuer": "Atlas.UserService",
    "Audience": "Atlas.Client"
  },
  "OpenSearch": {
    "Url": "http://OPENSEARCH_HOST:9200"
  },
  "RabbitMQ": {
    "HostName": "RABBITMQ_HOST",
    "Port": 5672,
    "UserName": "RABBITMQ_USER",
    "Password": "RABBITMQ_PASSWORD",
    "VirtualHost": "/"
  },
  "FrontendUrl": "http://localhost:5004"
}
